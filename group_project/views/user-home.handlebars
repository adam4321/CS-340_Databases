{{!  Description:  This view is the component that renders the homepage. This is the  }}
{{!                component where the bug list will be displayed.                    }}

<title>Bug Dashboard</title>
  <link rel="stylesheet" href="https://code.getmdl.io/1.1.3/material.orange-indigo.min.css">
  <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
  <script defer src="https://code.getmdl.io/1.1.3/material.min.js"></script>
  <link type="image/x-icon" rel="shortcut icon" href="favicon-1.ico"/>
  <link type="text/css" rel="stylesheet" href="user-home.css">  
      
<!-- Container for the dashboard -->
<div class="mdl-card mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-cell--12-col-tablet mdl-cell--12-col-desktop" id="main-box">
  <div id="dashboard-top-bar" class="mdl-card__title mdl-color-text--white">
    <h2 class="mdl-card__title-text">Bug Dashboard</h2>
  </div>
  <div class="mdl-card__supporting-text mdl-color-text--grey-600">  
    <h1>OSU MySQL Results: {{results}}</h1>  
    <br>
    <img alt="bar chart" src="chart.png" id="user-history-chart">
    <br>
  </div>
</div>